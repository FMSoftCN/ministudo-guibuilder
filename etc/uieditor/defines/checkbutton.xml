<window class="CheckButton" extends="Button">

	<set-property name="Class" value="checkbutton"/>
	<set-property name="GroupId" value="-1"/>
	<set-property name="Checkable" value="True"/>
	<set-property name="ThreeDCheck" value="False"/>
	<default-property name="Transparent" value="False"/>
	<default-property name="Width" value="160"/>
	<default-property name="Height" value="25"/>
	<default-property name="Transparent" value="False"/>
	<default-property name="Align" value="Left"/>
	<default-property name="AutoCheck" value="True"/>
	
	<event>
		<handler>onStateChanged</handler>
		<code>NCS_NOTIFY_CODE(NCSN_BUTTON_STATE_CHANGED)</code>
	</event>
</window>

